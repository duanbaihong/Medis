{
  "name": "medis",
  "description": "GUI For Redis Dbh888 ",
  "productName": "Medis",
  "version": "0.6.4",
  "electronVersion": "3.1.6",
  "license": "MIT",
  "author": "duanbaihong <duanbaihong@qq.com> (http://duanbaihong.com)",
  "main": "server/main.js",
  "scripts": {
    "build": "webpack --progress --color ",
    "watch": "yarn build -- --watch",
    "electron": "electron . ",
    "debug": "electron --debug=5858 .",
    "lint": "xo client/**/*.{js,jsx}",
    "pack": "yarn build  -p --config webpack.production.config.js && node bin/pack.js",
    "ppp": "node bin/pack.js",
    "release": "NODE_ENV=production ./bin/release"
  },
  "xo": {
    "extends": "xo-react/space",
    "semicolon": false,
    "ignore": [
      "client/vendors/**",
      ".git",
      "client/photon/**"
    ],
    "envs": [
      "browser"
    ],
    "globals": [
      "showModal"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "unicorn/filename-case": "off",
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before"
          }
        }
      ],
      "new-cap": "off",
      "import/no-unassigned-import": "off",
      "import/default": "off",
      "import/prefer-default-export": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/duanbaihong/Medis.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "fixed-data-table-contextmenu": "^1.7.2",
    "ioredis": "^4.6.2",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "redis-commands": "^1.0.1",
    "ssh2": "^0.8.2"
  },
  "devDependencies": {
    "ajv": "^6.5.2",
    "ajv-keywords": "^3.2.0",
    "asar": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "codemirror": "^5.25.2",
    "conventional-github-releaser": "^0.5.3",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^1.1.5",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.15-beta.0",
    "electron": "^3.1.6",
    "electron-osx-sign": "^0.4.4",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.2.5",
    "eslint-config-xo": "^0.18.1",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-plugin-react": "^7.12.4",
    "github": "^0.2.4",
    "hoek": "^5.0.4",
    "human-format": "^0.5.0",
    "immutable": "^4.0.0-rc.12",
    "json-editor": "^0.7.28",
    "jsonlint": "^1.6.2",
    "jsx-loader": "^0.13.2",
    "lint": "^1.1.2",
    "minimatch": "^3.0.4",
    "msgpack5": "^3.3.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.4",
    "react-addons-css-transition-group": "^15.6.2",
    "react-codemirror": "^1.0.0",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.4",
    "react-draggable": "^3.2.1",
    "react-redux": "^6.0.1",
    "react-split-pane": "^0.1.63",
    "redis-splitargs": "^1.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.0.3",
    "reselect": "^3.0.1",
    "sass-loader": "^7.1.0",
    "sortablejs": "^1.4.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "xo": "^0.22.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
